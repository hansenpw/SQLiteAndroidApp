<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hansen.sqlitetest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.hansen.sqlitetest.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.hansen.sqlitetest.CreateOrEditActivity"
            android:parentActivityName="com.example.hansen.sqlitetest.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.hansen.sqlitetest.MainActivity" />
        </activity>
        <activity
            android:name="com.example.hansen.sqlitetest.CurrencyEditActivity"
            android:parentActivityName="com.example.hansen.sqlitetest.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.hansen.sqlitetest.MainActivity"/>
        </activity>
    </application>

</manifest>